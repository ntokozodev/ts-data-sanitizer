{
  "name": "ts-data-sanitizer",
  "version": "1.1.1",
  "description": "A TypeScript utility for sanitizing data by removing empty values, functions, and nulls",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-push": "sh .husky/pre-push"
    }
  },
  "keywords": [
    "typescript",
    "sanitize",
    "data",
    "clean",
    "utility"
  ],
  "author": "gabzitdev",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitest/coverage-v8": "^1.0.0",
    "husky": "^9.1.7",
    "tsup": "^8.5.0",
    "typescript": "^5.0.4",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabzitdev/ts-data-sanitizer.git"
  }
}
